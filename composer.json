{
  "name": "andersundsehr/rah",
  "license": "proprietary",
  "type": "project",
  "require": {
    "php": ">=8.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-zip": "*",
    "phpstan/phpdoc-parser": "^2.1",
    "symfony/console": "~7.2.5",
    "symfony/dotenv": "~7.2.0",
    "symfony/finder": "~7.2.2",
    "symfony/flex": "^2.5",
    "symfony/framework-bundle": "~7.2.5",
    "symfony/http-client": "~7.2.4",
    "symfony/mime": "~7.2.4",
    "symfony/property-access": "~7.2.3",
    "symfony/property-info": "~7.2.5",
    "symfony/runtime": "~7.2.3",
    "symfony/serializer": "~7.2.5",
    "symfony/twig-bundle": "~7.2.0",
    "symfony/validator": "~7.2.5",
    "symfony/yaml": "~7.2.5",
    "thecodingmachine/safe": ">=3.0.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6.22",
    "pluswerk/grumphp-config": "^10.0",
    "symfony/browser-kit": "~7.2.4",
    "symfony/css-selector": "~7.2.0",
    "symfony/maker-bundle": "^1.62.1",
    "symfony/phpunit-bridge": "^7.2",
    "thecodingmachine/phpstan-safe-rule": ">=1.4"
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php82": "*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "php-http/discovery": true,
      "phpro/grumphp": true,
      "phpstan/extension-installer": true,
      "pluswerk/grumphp-config": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "bump-after-update": true,
    "sort-packages": true
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.2.*"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts",
      "composer normalize"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "test": "php -d pcov.enabled=1 vendor/bin/phpunit"
  }
}
